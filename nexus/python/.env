# Python 环境
PYTHON UNBUFFERED=1

# OpenStack 认证配置
OS_AUTH_URL=http://10.0.30.111:5000/v3
OS_USERNAME=admin
OS_PASSWORD=password
OS_PROJECT_NAME=admin
OS_USER_DOMAIN_NAME=Default
OS_PROJECT_DOMAIN_NAME=Default
OS_REGION_NAME=RegionOne

# 服务发现配置
OPENSTACK_NAMESPACE=openstack
PUBLIC_SERVICE_NAME=public-openstack
FALLBACK_TARGET=keystone.openstack.svc.cluster.local

# 功能开关
DNS_ENABLED=true
DISCOVERY_DEBUG=true
CLEANUP_TEMP_FILES=true

# 代理配置
PROXY_HTTP_PORT=80
PROXY_HTTPS_PORT=443
PROXY_SSL_ENABLED=true
PROXY_CONNECT_TIMEOUT=600s
PROXY_SEND_TIMEOUT=600s
PROXY_READ_TIMEOUT=600s
PROXY_CLIENT_MAX_BODY_SIZE=0

# 本地开发配置（如果使用 DevStack）
# OS_AUTH_URL=http://localhost:5000/v3
# OS_USERNAME=demo
# OS_PASSWORD=password